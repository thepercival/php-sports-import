<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SportsImport\Attachers\SportAttacher" table="externalSports">
    <many-to-one field="importable" target-entity="Sports\Sport">
      <join-columns>
        <join-column name="importableObjectId" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
      </join-columns>
    </many-to-one>
    <unique-constraints>
      <unique-constraint columns="externalSourceId,importableObjectId" name="main_idx" />
    </unique-constraints>
  </entity>
</doctrine-mapping>