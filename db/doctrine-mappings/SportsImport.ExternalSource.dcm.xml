<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SportsImport\ExternalSource" table="externalSystems" repository-class="SportsImport\ExternalSource\Repository">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" column="name" type="string" length="50" nullable="false" unique="true"/>
    <field name="website" column="website" type="string" length="255" nullable="true" unique="false"/>
    <field name="username" column="username" type="string" length="50" nullable="true" unique="false"/>
    <field name="password" column="password" type="string" length="50" nullable="true" unique="false"/>
    <field name="apiurl" column="apiurl" type="string" length="255" nullable="true" unique="false"/>
    <field name="apikey" column="apikey" type="string" length="255" nullable="true" unique="false"/>
  </entity>
</doctrine-mapping>